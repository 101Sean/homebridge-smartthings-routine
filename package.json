{
  "name": "homebridge-smartthings-childbridge",
  "version": "1.0.7",
  "description": "SmartThings Routine as a child HomeKit Bridge with a Switch accessory",
  "main": "index.js",
  "keywords": [
    "homebridge-plugin",
    "homebridge-platform",
    "smartthings",
    "routine",
    "switch",
    "childbridge"
  ],
  "engines": { "node": ">=14.17.0" },
  "dependencies": { "axios": "^1.4.0" },
  "peerDependencies": { "homebridge": ">=1.5.0" },
  "homebridge": {
    "platforms": [
      {
        "platform": "StRoutinePlatform",
        "name": "Good Bridge",
        "routineId": "YOUR_SCENE_ID",
        "token": "YOUR_TOKEN",
        "switchName": "Run Routine"
      }
    ]
  }
}