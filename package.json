{
  "name": "homebridge-smartthings-routine",
  "keywords": ["homebridge-plugin","homebridge-platform"],
  "version": "1.0.3",
  "description": "SmartThings Routine as TV-icon HomeKit Bridge",
  "main": "index.js",
  "engines": { "node": ">=14.17.0" },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "peerDependencies": {
    "homebridge": ">=1.5.0"
  },
  "homebridge": {
    "platforms": [
      {
        "platform": "StRoutinePlatform",
        "name": "SmartThings Routine Bridge",
        "routineId": "YOUR_ROUTINE_ID",
        "token": "YOUR_SMARTTHINGS_TOKEN"
      }
    ]
  }
}
